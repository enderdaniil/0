#include <stdio.h>
int main()
{

	int i, b, j, n;
	int a[100];
	printf("Input count of elements\n");        /*Добавил для удобства*/
	scanf_s("%d", &n);              /*в "scanf"  и "scanf_s" нельзя ставить пробелы здесь была ошибка: "scanf_s("%d ", &n);" пробел после %d нельзя ставить*/
	printf("Input array\n");        /*Добавил для удобства*/
	for (i = 1; i <= n; i++)
	{
		scanf_s("%d", &a[i]);     /*в "scanf"  и "scanf_s" нельзя ставить пробелы здесь была ошибка: "scanf_s("%d ", &a[i]);" пробел после %d нельзя ставить*/
	}
	for (i = 1; i <= (n - 1); i++)
	{
		for (j = 1; j <= (n - i); j++)
		{
			if (a[j] > a[j + 1])
			{
				b = a[j];
				a[j] = a[j + 1];
				a[j + 1] = b;
			}
		}
	}

	printf("Sorted array: ");    /*Добавил для удобства*/
	for (i = 1; i <= n; i++)
	{
		printf("%d ", a[i]);
	}
	return 0;
}
